@model SaphirCongesCore.Models.Conges

@{
    ViewBag.Title = "Supprimer";
}
<h2> Supprimer</h2>
<h3> Êtes vous certain de vouloir supprimer ceci?</h3>
<div>
    <h4> Congés </h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Date de début: ")
        </dt>
        <dd>
            @Html.DisplayNameFor(model => model.StartDate)
        </dd>
        <dt>
            @Html.DisplayName("Date de fin: ")
        </dt>
        <dd>
            @Html.DisplayNameFor(model => model.EndDate)
        </dd>
        <dt>
            @Html.DisplayName("Nombres de jours: ")
        </dt>
        <dd>
            @Html.DisplayNameFor(model => model.NoOfDays)
        </dd>
        <dt>
            @Html.DisplayName("Statut: ")
        </dt>
        <dd>
            @Html.DisplayNameFor(model => model.Statut)
        </dd>
    </dl>
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()
            <div class="form-actions no-color">
                <input type="submit" value="Supprimer" class="btn btn-default" />|
                @Html.ActionLink("Retour", "Index")
            </div>
        }
</div>